}

<!DOCTYPE html>
<html lang="en">
<body>
    <!-- ======= Intro Section ======= -->
    <section id="intro">
        <div class="intro-container">
            <div id="introCarousel" class="carousel  slide carousel-fade" data-ride="carousel">

                <ol class="carousel-indicators"></ol>

                <div class="carousel-inner" role="listbox">
                    <div class="carousel-item active">
                        <div class="carousel-background"><img src="~/Content/img/intro-carousel/Panda.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Creatures of China</h2> <!-----------------------text edits-------------->
                                <p>Come see our newest exhibit, on loan from the Beijing Zoo!</p>

                                <a class="btn-get-started scrollto" href="@Url.Action("Index", "Home")">Explore</a>

                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="~/Content/img/intro-carousel/Bird.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Avians of the Americas</h2> <!-----------------------text edits-------------->
                                <p style="text-align:left">Our most popular exhibit yet, with the residents easily seen in our special arboreal setup!</p>
                                <a class="btn-get-started scrollto" href="@Url.Action("Index", "Home")">Explore</a>
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="~/Content/img/intro-carousel/Fish.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Coral Curios</h2>
                                <p>Visit our carefully-maintained coral aquarium!</p>
                                <a class="btn-get-started scrollto" href="@Url.Action("Index", "Home")">Explore</a>
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="~/Content/img/intro-carousel/flock.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>The Pink Posse</h2>
                                <p>Become friends with the largest flamingo flock ever kept on display!</p>
                                <a class="btn-get-started scrollto" href="@Url.Action("Index", "Home")">Explore</a>
                            </div>
                        </div>
                    </div>

                    <div class="carousel-item">
                        <div class="carousel-background"><img src="~/Content/img/intro-carousel/fox.jpg" alt=""></div>
                        <div class="carousel-container">
                            <div class="carousel-caption">
                                <h2>Red Rioters</h2>
                                <p>Visit our rambunctious red pandas Rocket and Riot!</p>
                                <a class="btn-get-started scrollto" href="@Url.Action("Index", "Home")">Explore</a>
                            </div>
                        </div>
                    </div>
                </div>

                <a class="carousel-control-prev" href="#introCarousel" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon ion-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>

                <a class="carousel-control-next" href="#introCarousel" role="button" data-slide="next">
                    <span class="carousel-control-next-icon ion-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </section><!-- End Intro Section -->

        <main id="main">
